<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>RunAtLoad</key>
	<false/>
	<key>Label</key>
	<string>net.earthlingsoft.TimeMachineWaker</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/sh</string>
		<string>-c</string>
		<string>echo &quot;Waking Time Machine.&quot;; defaults write /Library/Preferences/com.apple.TimeMachine AutoBackup -bool YES; /System/Library/CoreServices/backupd.bundle/Contents/Resources/backupd-helper -wake</string>
	</array>
	<key>StartCalendarInterval</key>
	<dict>
		<key>Hour</key>
		<integer>8</integer>
		<key>Minute</key>
		<integer>0</integer>
	</dict>
</dict>
</plist>
